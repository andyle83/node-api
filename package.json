{
  "name": "node-api",
  "version": "1.0.0",
  "description": "node api example",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "start": "cross-env ENV_FILE=./config/.env.prod node -r tsconfig-paths/register ./bin/app.js",
    "dev": "cross-env ENV_FILE=./config/.env.dev ts-node -r tsconfig-paths/register ./src/app.ts",
    "test:unit": "cross-env ENV_FILE=./config/.env.test jest"
  },
  "repository": "git@github.com:andyle83/node-api.git",
  "author": "andy le",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.11",
    "@types/validator": "^13.6.3",
    "@types/yamljs": "^0.2.31",
    "bcrypt": "^5.0.1",
    "connect": "^3.7.0",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.12.15",
    "mongodb-memory-server": "^7.3.4",
    "mongoose": "^5.13.5",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.6",
    "swagger-routes-express": "^3.3.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0"
  }
}
